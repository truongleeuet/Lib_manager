<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>lib_manager</display-name>
  <servlet>
  	<servlet-name>ControllerTmp</servlet-name>
  	<servlet-class>controler.Controllertmp</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerTmp</servlet-name>
  	<url-pattern>/jsp/controllerTmp</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Controler</display-name>
    <servlet-name>Controler</servlet-name>
    <servlet-class>controler.Controler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controler</servlet-name>
    <url-pattern>/jsp/controler</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>CharsetFilter</filter-name>
    <filter-class>controler.CharsetFilter</filter-class>
    <init-param>
      <param-name>requestEncoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharsetFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <page-encoding>UTF-8</page-encoding>
    </jsp-property-group>
  </jsp-config>
</web-app>